api/auth/login\", reqBody, {\n            headers: {\n                \"Content-Type\": \"application/json\"\n            }\n        })\n        .then(response =>{\n            setJwt(response.headers.authorization);\n            console.log(jwt);\n            console.log(response.data);\n            window.location.href = \"/cart\";\n        })\n        .catch(err =>{\n            alert(\"Invalid username or password\");\n        })\n    }\n\n  return (\n    <>\n      <Container>\n        <Row className=\"justify-content-center mt-5\">\n          <Col md=\"8\" lg=\"6\">\n            <Form.Group className=\"mb-3 text-center\" controlId=\"username\">\n              <Form.Label className=\"fs-4\">Email</Form.Label>\n              <Form.Control\n                type=\"email\"\n                size=\"lg\"\n                value={username}\n                onChange={(e) => setUsername(e.target.value)}\n              />\n            </Form.Group>\n          </Col>\n        </Row>\n\n        <Row className=\"justify-content-center\">\n          <Col md=\"8\" lg=\"6\">\n            <Form.Group className=\"mb-3 text-center\" controlId=\"password\">\n              <Form.Label className=\"fs-4\">Password</Form.Label>\n              <Form.Control\n                type=\"password\"\n                size=\"lg\"\n                onChange={(e) => setPassword(e.target.value)}\n              />\n            </Form.Group>\n          </Col>\n        </Row>\n\n        <Row className=\"justify-content-center\">\n          <Col md=\"8\" lg=\"6\" className=\"mt-2 d-flex flex-column gap-3 flex-md-row justify-content-end\">\n            <Button\n              id=\"submit\"\n              type=\"button\"\n              size=\"lg\"\n              onClick={sendLoginRequest}\n            >\n              Login\n            </Button>\n            <Button\n              id=\"submit\"\n              type=\"button\"\n              size=\"lg\"\n              onClick={() => {\n                window.location.href = \"/\";\n              }}\n              variant=\"secondary\"\n            >\n              Exit\n            </Button>\n          </Col>\n        </Row>\n        <div className='text-center mt-3'>\n        <a href=\"/register\" className=\"link-primary\">Create new account</a>\n        </div>\n      </Container>\n    </>\n  )\n}\n\nexport default Login","/Users/y_ryo0o0o0/Desktop/OnlineShopping/frontend/src/components/NavBar.js",["125","126","127","128","129","130"],[],"import React, {useState, useEffect} from \"react\";\nimport * as ReactBootStrap from \"react-bootstrap\";\nimport \"font-awesome/css/font-awesome.min.css\";\nimport { useLocalState } from \"../util/useLocalStroge\";\nimport axios from \"axios\";\nimport axiosService from \"../service/AxiosService\";\n\nconst NavBar = () => {\n\n  const [jwt, setJwt] = useLocalState(\"\", \"jwt\");\n\n  return (\n    <div>\n      <ReactBootStrap.Navbar bg=\"white\" expand=\"md\" className=\"shadow-sm\">\n        <ReactBootStrap.Container>\n          <ReactBootStrap.Navbar.Brand href=\"/home\" className=\"fs-4 py-3\">\n          <i className=\"fa fa-coffee me-2 fa-sm\"></i>\n            Starbucks\n          </ReactBootStrap.Navbar.Brand>\n          <ReactBootStrap.Navbar.Toggle aria-controls=\"basic-navbar-nav\" />\n          <ReactBootStrap.Navbar.Collapse id=\"basic-navbar-nav\">\n            <ReactBootStrap.Nav className=\"mx-auto\">\n              <ReactBootStrap.Nav.Link href=\"/menu\">\n              Menu\n              </ReactBootStrap.Nav.Link>\n              <ReactBootStrap.Nav.Link href=\"/about\">\n                About\n              </ReactBootStrap.Nav.Link>\n              <ReactBootStrap.Nav.Link href=\"/contact\">\n                Contact\n              </ReactBootStrap.Nav.Link>\n            </ReactBootStrap.Nav>\n            <div className=\"buttons\">\n              <a href=\"/login\" className=\"btn btn-outline-dark\">\n                <i className=\"fa fa-sign-in me-2\"></i> Login\n